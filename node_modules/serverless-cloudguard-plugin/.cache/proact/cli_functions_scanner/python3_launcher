#!/usr/local/bin/python3

import sys
import json
# first arg is at place 1 (0: filename. 1: arg)
json_input_file_name = str(sys.argv[1])
print("json_input_file_name: " + json_input_file_name)
with open(json_input_file_name, "r") as f:
    event = json.load(f)
print("event: " + json.dumps(event))

from function_code_analysis_python3 import function_code_analysis_python
event = function_code_analysis_python.lambda_handler(event, {})

print("event After: " + json.dumps(event))
with open(json_input_file_name, "w") as f:
    json.dump(event, f)
